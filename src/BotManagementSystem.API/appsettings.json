{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": "YOUR_APPLICATION_INSIGHTS_CONNECTION_STRING"
  },
  "Jwt": {
    "Key": "YOUR_JWT_SECRET_KEY_HERE_AT_LEAST_32_CHARACTERS_LONG",
    "Issuer": "BotManagementSystem",
    "Audience": "BotManagementSystem.API",
    "ExpiryInMinutes": 1440
  }
}
